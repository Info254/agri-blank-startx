# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
VITE_API_URL=your_api_url_here
VITE_WS_URL=your_websocket_url_here

# Authentication
VITE_AUTH_TOKEN_KEY=auth_token
VITE_REFRESH_TOKEN_KEY=refresh_token
VITE_TOKEN_EXPIRY_KEY=token_expiry

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_LOGGING=true
VITE_ENABLE_OFFLINE_MODE=true

# Third-party Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_MPESA_CONSUMER_KEY=your_mpesa_consumer_key
VITE_MPESA_CONSUMER_SECRET=your_mpesa_consumer_secret
VITE_MPESA_PASSKEY=your_mpesa_passkey
VITE_MPESA_SHORTCODE=your_mpesa_shortcode

# Notification Services
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Environment
NODE_ENV=development
VITE_APP_ENV=development

# Debugging
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug

# App Configuration
VITE_APP_NAME="Soko Connect"
VITE_APP_VERSION=1.0.0
VITE_APP_BUILD_NUMBER=100

# Security
VITE_ENABLE_SSL_PINNING=true
VITE_ENABLE_BIOMETRIC_AUTH=false
VITE_SESSION_TIMEOUT=1800000

# Localization
VITE_DEFAULT_LOCALE=en
VITE_SUPPORTED_LOCALES=en,sw

# Pagination
VITE_DEFAULT_PAGE_SIZE=10
VITE_MAX_PAGE_SIZE=100

# Caching
VITE_CACHE_TTL=3600000
VITE_OFFLINE_CACHE_TTL=86400000
